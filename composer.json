{
    "name": "stephen/vakkertech.com",
    "description": "Custom WP Theme for VakkerTech.com",
    "type": "project",
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "sc0z",
            "email": "sjscoza7@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "require": {
        "wpackagist-plugin/classic-editor":"1.5",
        "wpackagist-plugin/cmb2":"2.6.0",
        "wpackagist-plugin/contact-form-7":"5.1.1",
        "wpackagist-plugin/flamingo":"1.9",
        "wpackagist-plugin/wp-migrate-db":"1.0.11",
        "wpackagist-theme/twentynineteen":"1.4"
    },
    "extra": {
        "installer-paths": {
            "wp-content/plugins/{$name}/": ["type:wordpress-plugin"],
            "wp-content/themes/{$name}/": ["type:wordpress-theme"]
        }
    }
}
